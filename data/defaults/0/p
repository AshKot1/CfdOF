%{%(boundaries/%(0%)/BoundarySubtype%)
%:totalPressure
    %(0%)
    {
        type        totalPressure;
        p0          uniform %(boundaries/%(0%)/Pressure%);
        value       $internalField;
    }
%:staticPressure
    %(0%)
    {
        type        fixedValue;
        value       uniform %(boundaries/%(0%)/KinematicPressure%);
    }
%:default
%[0/_boundary_zeroGradient%]
%}
%:outlet
%{%(boundaries/%(0%)/BoundarySubtype%)
%:staticPressure
    %(0%)
    {
        type        fixedValue;
        value       uniform %(boundaries/%(0%)/KinematicPressure%);
    }
%:default
%[0/_boundary_zeroGradient%]
%}
%:open
    %(0%)
    {
        type        totalPressure;
        p0          uniform %(boundaries/%(0%)/Pressure%);
        value       $internalField;
    }
%:constraint
%[0/_boundary_constraint%]
%:baffle
%{%(boundaries/%(0%)/BoundarySubtype%)
%:porousBaffle
%{%(0%)_master %(0%)_slave
    %(0%)
    {
        type        porousBafflePressure;
        patchType   cyclic;
        length      1.0;
        I           %(boundaries/%(1%)/PressureDropCoeff%);
        D           0;
        value       $internalField;
    }
%}
%}
%}

